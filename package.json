{
	"name": "svg-fold",
	"publisher": "misbahansori",
	"displayName": "svg-fold",
	"description": "Fold SVG tag",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/misbahansori/svg-fold"
	  },
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:svg-fold.foldAllSVGTag"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "svg-fold.foldAllSVGTag",
				"title": "Fold all SVG tag"
			}
		],
		"keybindings": [
            {
                "command": "svg-fold.foldAllSVGTag",
                "key": "ctrl+k ctrl+9",
                "mac": "cmd+k cmd+9"
            }
        ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
